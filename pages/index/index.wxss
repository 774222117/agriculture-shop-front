/* 主体内容 */
/* 背景图片 轮播 公告 品类 大层盒子 */
.mainBox{
  width: 100%;
  height: 632rpx;
  position: relative
}
/* 背景图片层 */
.mainBox .topBgImgs{
  width: 100%;
  height: 384rpx;
  background: linear-gradient(180deg,#f37920 100%, #ffcd1c 100%);
  border-radius: 0 0 32rpx 32rpx;
  position: absolute;
  top: 0;
  z-index: -1;
}
/* 轮播 公告 品类 */
.mainBox .contents{
  width: calc(100% - 40rpx);
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  z-index: 2;
  margin: 0 auto;
}
/* 轮播 */
.mainBox .contents .banners{
  width: 100%;
  height: 228rpx;
  margin-bottom: 12rpx;
}
/* 公告 */
.mainBox .contents .notice{
  width: 100%;
  height: 60rpx;
  background-color: #fef2e6;
  margin-bottom: 12rpx;
  border-radius: 54rpx;
}
/* 公告单个元素 */
.notice .noticeItems{
  width: 100%;
  height: 100%;
  border-radius: 54rpx;
  display: flex;
  align-items: center;
}
/* 鲜丰汇公告 */
.notice .noticeItems .text1{
  font-size: 24rpx;
  font-family: PingFang SC, PingFang SC-Regular;
  font-weight: bold;
  color: rgba(0,0,0,0.85);
  margin-left: 26rpx;
}
.notice .noticeItems .text1 text{
  color: #91c43d;
  font-weight: bold;
}
/* 资质认证 */
.notice .noticeItems .text2{
  width: 136rpx;
  height: 40rpx;
  font-size: 24rpx;
  background-color: #91c43d;
  border-radius: 34rpx;
  color: #ffffff;
  font-weight: 400;
  text-align: center;
  line-height: 40rpx;
  margin-left: 10rpx;
}
/* 指定平台 */
.notice .noticeItems .text3{
  font-size: 24rpx;
  font-family: PingFang SC, PingFang SC-Regular;
  font-weight: 400;
  color: rgba(0,0,0,0.85);
  margin-left: 8rpx;
}
/* 品类 */
.mainBox .contents .category{
  width: 100%;
  height: 312rpx;
  background: #ffffff;
  border-radius: 16rpx;
  overflow: hidden
}
/* 品类包裹一层 */
