/* 顶部搜索页 */
.topSerach {
	width: 750rpx;
	height: 88rpx;
	opacity: 1;
	background: #ffffff;
	display: flex;
}
/* 搜索框 */
.serachInput {
	min-width: 702rpx;
	height: 60rpx;
	margin: 16rpx 0 0 24rpx;
	border: 2rpx solid #91c43d;
	border-radius: 202rpx;
	display: flex;
	align-items: center;
}
/* 搜索有结果 */
.haveresult {min-width: 610rpx;}
/* 搜索pic */
.serachInput .serachPic {
	width: 36rpx;
	height: 36rpx;
	margin-left: 32rpx;
}
.serachInput .serachPic image{width: 100%;height: 100%;display: block;}
/* 输入框 */
.serachInput .serachfor {
	width: 280rpx;
	height: 44rpx;
	margin-left: 8rpx;
}
/* 清理图标 */
.clear {
	width: 48rpx;
	height: 48rpx;
	margin: 20rpx 0 0 24rpx;
}
.clear image{width: 100%;height: 100%;display: block;}
/* 详情盒子 */
.containerBox-noresult {
	width: 750rpx;
	/* height: 1360rpx; */
	background: #ffffff;
	/* display: flex; */
	/* flex-direction: column; */
}
/* 历史搜索 */
.history {
	width: 128rpx;
	height: 48rpx;
	font-size: 32rpx;
	font-family: PingFang SC, PingFang SC-Semibold;
	font-weight: 600;
	text-align: left;
	color: rgba(0,0,0,0.85);
	line-height: 48rpx;
	margin: 28rpx 0 0 28rpx;
}
/* 历史搜索结果 */
.historyResult {
	width: 690rpx;
	min-height: 48rpx;
	margin: 28rpx 0 0 28rpx;
	display: flex;
	flex-wrap: wrap;
}
.historyResult .historyItem {
	width: 104rpx;
	height: 48rpx;
	background-color: rgba(60,60,67,0.06);
	border-radius: 24rpx;
	font-size: 24rpx;
	font-family: PingFang SC, PingFang SC-Regular;
	font-weight: 400;
	text-align: center;
	color: rgba(60,60,67,0.65);
	line-height: 40rpx;
	margin: 0 16rpx 16rpx 0;
}
/* 热门搜索 */
.hot {
	width: 128rpx;
	height: 48rpx;
	font-size: 32rpx;
	font-family: PingFang SC, PingFang SC-Semibold;
	font-weight: 600;
	text-align: LEFT;
	color: rgba(0,0,0,0.85);
	line-height: 48rpx;
	margin: 56rpx 0 0 28rpx;
}
/* 热门搜索记录 */
.hotResult {
	width: 690rpx;
	min-height: 48rpx;
	margin: 28rpx 0 0 28rpx;
}
.hotResult .hotItem {
	width: 104rpx;
	height: 48rpx;
	background-color: rgba(60,60,67,0.06);
	border-radius: 24rpx;
	font-size: 24rpx;
	font-family: PingFang SC, PingFang SC-Regular;
	font-weight: 400;
	text-align: center;
	color: rgba(60,60,67,0.65);
	line-height: 40rpx;
}
/* 被选中 */
.ischange{display: block;}
/* 不被选中 */
.unchange{display: none;}

/* 有搜索结果 */
.containerBox-haveresult {
	width: 750rpx;
	/* height: 1360rpx; */
	background: #ffffff;
	display: flex;
	flex-direction: column;
}
/* 排序盒子 */
.sortBox {
	width: 750rpx;
	height: 90rpx;
	display: flex;
	align-items: center;
}
/* 默认排序 */
.sortBox .defaultSort {
	width: 96rpx;
	height: 40rpx;
	font-size: 24rpx;
	font-family: PingFang SC, PingFang SC-Regular;
	font-weight: 400;
	text-align: center;
	line-height: 40rpx;
	margin-left: 24rpx;
}
/* 销量排序 */
.sortBox .sellNum {
	width: 48rpx;
	height: 40rpx;
	opacity: 1;
	font-size: 24rpx;
	font-family: PingFang SC, PingFang SC-Regular;
	font-weight: 400;
	text-align: center;
	line-height: 40rpx;
	margin-left: 232rpx;
}
/* 价格排序 */
.sortBox .price {
	width: 48rpx;
	height: 40rpx;
	opacity: 1;
	font-size: 24rpx;
	font-family: PingFang SC, PingFang SC-Regular;
	font-weight: 400;
	text-align: center;
	line-height: 40rpx;
	margin-left: 250rpx;
}
/* 价格排序图 */
.sortBox .pricePic {
	width: 24rpx;
	height: 24rpx;
	opacity: 1;
	margin-left: 4rpx;
}
.sortBox .pricePic image {width: 100%;height: 100%;display: block;}
/* 被选中 */
.isClick {color: rgba(0,0,0,0.85);}
/* 未被选中 */
.unClick {color: rgba(60,60,67,0.45);}

/* 更多的内容区域 */
.moreContents{
  width: calc(100% - 40rpx);
  margin: 0 auto;
  /* display: flex;
  flex-wrap: wrap;
  justify-content: space-between; */
}
.moreContents .left{
  float: left;
}
.moreContents .right{
  float: right;
}
/* 单个商品元素 */
.moreContents .commodityItem{
  width: 346rpx;
  /* height: 496rpx; */
  background-color: #ffffff;
  border-radius: 16rpx;
  overflow: hidden;
  box-shadow: 0px 2rpx 4rpx -4rpx rgba(0,0,0,0.16); 
  padding-bottom: 20rpx;
  margin-bottom: 20rpx;
}
/* 商品图片 */
.commodityItem .itemImgs{
  width: 306rpx;
  height: 306rpx;
  box-shadow: 0px 2rpx 4rpx -4rpx rgba(0,0,0,0.16); 
  margin: 20rpx 20rpx 10rpx 20rpx;
  margin-bottom: 8rpx;
}
/* 商品文字 */
.commodityItem .itemTitle{
  width:  calc(100% - 40rpx);
  /* height: 88rpx; */
  font-size: 28rpx;
  font-family: PingFang SC, PingFang SC-Regular;
  font-weight: 400;
  color: #2b2b2b;
  margin: 0 auto;
  text-shadow: 0px 2px 4px -4px rgba(0,0,0,0.16); 
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}
/* 商品文字 帮促 */
.commodityItem .itemTitle .helpPoor{
  width: 84rpx;
  height: 40rpx;
  padding: 0 20rpx;
  border: 2rpx solid #91c43d;
  border-radius: 10rpx;
  box-shadow: 0rpx 2rpx 4rpx -4rpx rgba(0,0,0,0.16); 
  font-size: 22rpx;
  font-family: PingFang SC, PingFang SC-Semibold;
  font-weight: 600;
  text-align: center;
  color: #91c43d;
  line-height: 40rpx;
}
/* 价格折扣 */
.commodityItem .priceDiscount{
  /* width: 232rpx; */
  width: max-content;
  height: 44rpx;
  margin:14rpx 0 16rpx 20rpx;
  background-color: #FEF0E7;
  display: flex;
}
.commodityItem .priceDiscount .leftPrice{
  height: 100%;
  display: flex;
  align-items: center;
  position: relative;
}
.commodityItem .priceDiscount .leftPrice .price1{
  margin-left: 8rpx;
  position: relative;
  z-index: 1;
  padding-right: 8rpx;
}
/* 商品价格 人民币符号 */
.commodityItem .priceDiscount .leftPrice .price1 .text1{
  font-size: 24rpx;
  font-family: PingFang SC, PingFang SC-Regular;
  font-weight: 400;
  color: #ffffff;
  line-height: 40rpx;
  text-shadow: 0rpx 2rpx 4rpx -4rpx rgba(0,0,0,0.16); 
}
/* 商品价格 价格 */
.commodityItem .priceDiscount .leftPrice .price1 .text2{
  font-size: 36rpx;
  font-family: PingFang SC, PingFang SC-Semibold;
  font-weight: 600;
  color: #ffffff;
  line-height: 52rpx;
  text-shadow: 0rpx 2rpx 4rpx -4rpx rgba(0,0,0,0.16); 
  margin-left: -4rpx;
}

.commodityItem .priceDiscount .leftPrice .price2{
  font-size: 24rpx;
  color: rgba(255,255,255,0.85);
  margin-left: 14rpx;
  margin-right: 20rpx;
  position: relative;
  z-index: 1;
  text-decoration: line-through;
}

.bgImgs{
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 0;
}
.commodityItem .priceDiscount .rightDiscount{
  width: 76rpx;
  height: 100%;
  font-size: 24rpx;
  font-family: PingFang SC, PingFang SC-Regular;
  font-weight: 400;
  color: #f37920;
  padding: 0 8rpx;
  line-height: 44rpx;
}
/* 商品价格 与 添加商品  包裹层*/
.commodityItem .itemPriceAdd{
  width:  calc(100% - 40rpx);
  height: 48rpx;
  margin: 0 auto ;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
/* 商品价格 */
.commodityItem .itemPriceAdd .price{
  font-size: 24rpx;
  font-family: PingFang SC, PingFang SC-Regular;
  font-weight: 400;
  color: rgba(60,60,67,0.65);
  line-height: 40rpx;
}

/* 添加商品 */
.commodityItem .itemPriceAdd .addItem{
  width: 48rpx;
  height: 48rpx;
}
/* 右边添加按钮 */
.btnBoxs{
	width: 162rpx;
	height: auto;
	display: flex;
	align-items: center;
	justify-content: space-between;
}
.btnBoxs.rightEnd{justify-content: flex-end;}
.addBtn,.cutBtn {
	width: 48rpx;
	height: 48rpx;
	cursor: pointer;
}
.addBtn .addBtnImg,.cutBtn .cutBtnImg {width: 100%;height: 100%;display: block;}
.imgSty{width: 100%;height:100%;display: block;}
.scroll-view_H{
  width: 100%;
  white-space: nowrap; 
}
.bgImgs{
  width: 100%;
  height: 44rpx;
  display: flex;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 0;
}
.bgImgs .bgImgs1{
  width: 100%;
  height: 44rpx;
  background: linear-gradient(90deg,#f39f20 0%, #f37920 100%);
  border-radius: 8rpx;
}
.bgImgs .bgImgs2{
  /* width: 34rpx;
  height: 44rpx; */
}
.scroll-view-item_H{
  display: inline-block;
}
/* 悬浮购物车 */
.suspensionShoppingCart{
  width: 96rpx;
  height: 96rpx;
  background-color: rgba(60,60,67,0.65);
  border: 2rpx solid rgba(255,255,255,0.45);
  border-radius: 50%;
  position: fixed;
  bottom: 94rpx;
  right: 28rpx;
}
.suspensionShoppingCart .icons{
  width: 48rpx;
  height: 48rpx;
  margin: 24rpx;
}
.suspensionShoppingCart .goodsNums{
  width: 36rpx;
  height: 36rpx;
  background: #ff3b30;
  border: 2rpx solid #ffffff;
  border-radius: 50%;
  font-size: 22rpx;
  font-family: PingFang SC, PingFang SC-Regular;
  font-weight: 400;
  text-align: CENTER;
  color: #ffffff;
  position: absolute;
  right: 0;
  top: 0;
}