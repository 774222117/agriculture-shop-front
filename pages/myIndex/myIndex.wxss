/* 我的订单 */
.myOrderBox {
	width: 710rpx;
	height: 256rpx;
	opacity: 1;
	background: #ffffff;
	border-radius: 16rpx;
	margin: 18rpx 0 0 20rpx;
	display: flex;
	flex-direction: column;
}
.myOrderBox .textOrder {
	width: 670rpx;
	height: 48rpx;
	margin: 20rpx 0 0 20rpx;
	display: flex;
	align-items: center;
	position: relative;
}
.textOrder .page1 {
	width: 96rpx;
	height: 40rpx;
	font-size: 24rpx;
