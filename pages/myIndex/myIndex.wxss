}
/* 用户名称 */
.top .userInfo .name {
	min-width: 204rpx;
	height: 52rpx;
	font-size: 36rpx;
	font-family: PingFang SC, PingFang SC-Semibold;
	font-weight: 600;
	text-align: left;
	color: rgba(0,0,0,0.85);
	line-height: 52rpx;
}
/* 用户类型 */
.top .userInfo .nameType {
	width: 96rpx;
	height: 40rpx;
	font-size: 24rpx;
	font-family: PingFang SC, PingFang SC-Regular;
	font-weight: 400;
	text-align: CENTER;
	color: rgba(60,60,67,0.65);
	line-height: 40rpx;
}
/* 联系客服 */
